initialize() {
    initializeTreeSeq();
	initializeMutationRate(5e-5);
	initializeMutationType("m1", 0.5, "f", -0.05);
	initializeGenomicElementType("g1", m1, 1.0);
	initializeGenomicElement(g1, 2499999, 2499999);
	initializeGenomicElement(g1, 7499999, 7499999);
    initializeRecombinationRate(1.0000000000287557e-7, 10000000);
}
1 early() {
